package com.google.analytics.debug
{

    public class Margin extends Object
    {
        public var top:int;
        public var bottom:int;
        public var left:int;
        public var right:int;

        public function Margin(top:int = 0, bottom:int = 0, left:int = 0, right:int = 0)
        {
            this.top = top;
            this.bottom = bottom;
            this.left = left;
            this.right = right;
            return;
        }// end function

    }
}
